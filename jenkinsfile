pipeline {
    agent any 
    stages{

        stage('Build'){
            steps{
                sh ''' sudo chmod 666 /var/run/docker.sock
                    docker-compose down --rmi all
                    docker-compose build
                    sudo docker login -u suban92 -p Xconcept7
                    sudo docker-compose push
                    '''
            }
        }
    }
}